                                        <div class="col col-sm-6 col-xs-12">
                                            <h3>Company</h3>
                                            <p><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span> @(Model.Address1 ?? ""), @(Model.Address2 ?? ""), @(Model.Address3 ?? "")</p>
                                            <p>@(Model.Zip ?? "") @(Model.City ?? "")</p>
